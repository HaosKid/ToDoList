        </div>
        <div class="box">
            <div class="item"><input type="checkbox"><p> <%= lista[7] %></p></div>
        </div>
        <div class="box">
            <div class="item"><input type="checkbox"><p> <%= lista[8] %></p></div>
        </div>
        <div class="box">
            <div class="item"><input type="checkbox"><p> <%= lista[9] %></p></div>
        </div>
        <form class="item" action="/" method="post">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
            <button type="submit" name="list" value=<%= value %> list="">+</button>
        </form>
        
        <footer>Pantazica Â© 2023</footer>







    </main>






    <script src="index.js"></script>
</body>
</html>