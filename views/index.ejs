      </div>

    <main>
        <div class="box">
            <h1 class="titlu"><%= titlu %></h1>
        </div>
        <div class="box">
            <div class="item"><input type="checkbox" onclick="check()"><p> <%= lista[0] %></p></div>
        </div>
        <div class="box">
            <div class="item"><input type="checkbox"><p> <%= lista[1] %></p></div>
